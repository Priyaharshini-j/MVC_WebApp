@{
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <h1>List of Students</h1>
    <h1>@ViewBag.info</h1>
    <div>
        @foreach (StudentModel student in ViewBag.students)
        {
            <table>
                <tr>
                    <td>
                        @student.Name
                    </td>
                    <td>
                        @student.Dept
                    </td>
                    <td>
                        @student.Id
                    </td>
                </tr>
            </table>
        }

    </div>
</div>